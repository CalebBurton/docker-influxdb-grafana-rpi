DOCKER_INFLUXDB_INIT_MODE=setup # Regular starts with v2
# DOCKER_INFLUXDB_INIT_MODE=upgrade # Upgrade from v1

# Upgrade vars
DOCKER_INFLUXDB_INIT_UPGRADE_V1_CONFIG=/home/influxdb/.influxdb1/config.conf
DOCKER_INFLUXDB_INIT_UPGRADE_V1_DIR=/var/lib/influxdb

# Common initialization vars
DOCKER_INFLUXDB_INIT_USERNAME=secureusername
DOCKER_INFLUXDB_INIT_PASSWORD=securepassword
DOCKER_INFLUXDB_INIT_ORG=my_org
DOCKER_INFLUXDB_INIT_BUCKET=home_assistant
DOCKER_INFLUXDB_INIT_ADMIN_TOKEN=admin_token

# INFLUXD_TLS_CERT=/etc/ssl/influxdb-selfsigned.crt
# INFLUXD_TLS_KEY=/etc/ssl/influxdb-selfsigned.key
